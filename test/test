package algoritmo1;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import static java.lang.System.in;

public class Algoritmo1 {


    public static void main(String[] args) throws IOException {
             
        InputStreamReader a= new InputStreamReader(in);
        BufferedReader rd= new BufferedReader(a);
        System.out.println("opcion \n 1. \n 2.Multiplicacion \n 3. ");
        int opcion=Integer.parseInt(rd.readLine());
        switch(opcion){
            case 1 :
                break;
            case 2:
                System.out.println("Multiplicacion dos numeros");
                    int n= Integer.parseInt(rd.readLine());
                    int m= Integer.parseInt(rd.readLine());
                    int Rmult=n*m;
                    System.out.println("Rmult");
                break;
            case 3:
                break;
        }
    }
    
}